    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-highlight": "^5.0.2",
    "remark-mdx-images": "^1.0.3",
    "remix": "^1.1.3",
    "tiny-invariant": "^1.2.0",
    "unified": "^10.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
